{
  "name": "snips-ai-assignment",
  "version": "1.0.0",
  "description": "Express + TypeScript backend for news enrichment with OpenAI",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "5.0.3",
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "globals": "^15.15.0",
    "prettier": "3.6.2",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
